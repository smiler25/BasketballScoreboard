<?xml version="1.0" encoding="utf-8"?>
<TableRow
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="start"
    android:clickable="false"
    android:longClickable="false"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

    <TextView
        android:id="@+id/protocol_l_player"
        style="@style/detailViewTableRowChild"
        android:layout_weight="1"
        />
    <TextView
        android:id="@+id/protocol_l_score"
        style="@style/detailViewTableRowChild"
        android:layout_weight="1"
        />
    <TextView
        android:id="@+id/protocol_period_time"
        style="@style/detailViewTableRowChild"
        android:layout_weight="1.5"
        />
    <TextView
        android:id="@+id/protocol_game_time"
        style="@style/detailViewTableRowChild"
        android:layout_weight="1.5"
        />
    <TextView
        android:id="@+id/protocol_r_player"
        style="@style/detailViewTableRowChild"
        android:layout_weight="1"
        />
    <TextView
        android:id="@+id/protocol_r_score"
        style="@style/detailViewTableRowChild"
        android:layout_weight="1"
        />
</TableRow>